#include <stdio.h>
#include <stdlib.h>

int main(void)
{
	float num1, num2;

	printf ("escreva dois numeros!\n");
	scanf ("%f %f", &num1, &num2);

	if (num1>num2)
		printf ("o numero %f e maior que o numero %f " num1,num2);
	if (num2>num1)
		printf ("o numero %f e maior que o numero %f " num2,num1);
	if (num1==num2)
		printf ("os numeros sao iguais");
	
	return 0;


}